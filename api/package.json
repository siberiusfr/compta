{
  "name": "compta-formatter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "format": "prettier --write \"**/*.{sql,yml,yaml,json}\"",
    "format:sql": "prettier --write \"**/*.sql\"",
    "format:yaml": "prettier --write \"**/*.{yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{sql,yml,yaml,json}\""
  },
  "devDependencies": {
    "prettier": "^3.4.2",
    "prettier-plugin-sql": "^0.19.2"
  }
}
